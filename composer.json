{
  "name" : "baki/php_boilerplate",
  "type": "project",
  "description": "Php project with laravel eloquent",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "keywords": ["template", "boilerplate", "php"],
  "require": {
    "php": "^7.1.3",
    "symfony/var-dumper": "4.4.*",
    "nesbot/carbon": "2.36.1",
    "illuminate/database" : "5.8.*",
    "illuminate/http" : "5.8.*",
    "vlucas/phpdotenv": "3.6.*",
    "illuminate/routing": "5.8.*",
    "illuminate/pagination" : "5.8.*",
    "baki/uploader": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "App\\Controllers\\": "app/controllers/",
      "App\\Models\\": "app/models/",
      "App\\Traits\\": "app/traits/",
      "App\\Providers\\": "app/providers/",
      "App\\Middleware\\": "app/middleware/"
    },
    "files": ["helper/helpers.php"]
  }
}
